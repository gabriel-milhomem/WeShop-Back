{
  "env": {
    "commonjs": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "standard",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "space-before-function-paren": "off",
    "no-console": "off",
    "semi": [2, "always"],
    "no-use-before-define": "off"
  }
}
